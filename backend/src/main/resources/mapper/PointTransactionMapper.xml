<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.kokoachino.mapper.PointTransactionMapper">

    <!-- 查询团队的点数流水 -->
    <select id="selectByTeamId" resultType="com.github.kokoachino.model.entity.PointTransaction">
        SELECT * FROM tw_point_transaction WHERE team_id = #{teamId} ORDER BY created_at DESC LIMIT #{limit}
    </select>

</mapper>
